<body>



    <%- include('partials/navigator') %>



    <div class="wb_contact">
        <div class="container p-4">
            <div class="row">
                <div class="col-md-4 mx-auto">
                    <div class="card text-center">
                        <div class="card-body text-dark display-5">
                            <div class="col-2-padding">
                                <div id="wb_contactText">
                                    <span
                                        style="color:#2E8B57;font-family:Arial;font-size:32px;line-height:37px;"><strong>Iniciar
                                            Sesion<br></strong></span><span
                                        style="color:#2E8B57;font-family:Arial;font-size:13px;line-height:16px;"><br></span><span
                                        style="color:#2E8B57;font-family:Arial;font-size:15px;line-height:16px;">Ingrese
                                        su correo
                                        electronico y contrase√±a</span>
                                </div>
                                <% if(message.length > 0) { %>
                                <div class="alert alert-danger"><%= message %></div>
                                <% } %>
                                <form action="/login" method="post">
                                    <input type="text" id="email"
                                        style="display:block;width:100%;height:34px;z-index:1;" name="username" value=""
                                        spellcheck="false" placeholder="Usuario">
                                    <input type="password" id="Editbox1"
                                        style="display:block;width:100%;height:34px;z-index:2;" name="password" value=""
                                        spellcheck="false" placeholder="contrase&#241;a">
                                    <input type="submit" class="btn btn-lg btn-success" value="Entrar (Ciudadano)"
                                        style="display:block;width:100%;font-size:20px;height:39px;z-index:3;">
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="overlay"></div>


